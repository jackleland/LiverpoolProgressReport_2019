journal = {Journal of Physics: Conference Series},
pages = {012002},
title = {{High volume fabrication of laser targets using MEMS techniques}},
url = {http://iopscience.iop.org/1742-6596/713/1/012002 http://stacks.iop.org/1742-6596/713/i=1/a=012002?key=crossref.1178e5a8a5aa3b13cece1da637682600},
