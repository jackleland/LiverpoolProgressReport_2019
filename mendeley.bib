number = {3},
pmid = {22462916},
title = {{Surface thermocouples for measurement of pulsed heat flux in the divertor of the Alcator C-Mod tokamak}},
url = {https://dspace.mit.edu/bitstream/handle/1721.1/94453/11ja042{\_}full.pdf?sequence=1},
volume = {83},
year = {2012}
}
