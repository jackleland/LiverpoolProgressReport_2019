file = {:home/jleland/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Matos et al. - Unknown - Deep learning for plasma tomography using the bolometer system at JET.pdf:pdf},
issn = {09203796},
journal = {Fusion Engineering and Design},
keywords = {Computed tomography,Deep learning,Neural networks,Plasma diagnostics},
pages = {18--25},
title = {{Deep learning for plasma tomography using the bolometer system at JET}},
url = {https://arxiv.org/pdf/1701.00322.pdf},
volume = {114},
year = {2017}
}
