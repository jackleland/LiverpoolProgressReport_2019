doi = {10.1063/1.4943527},
issn = {1070-664X},
journal = {Physics of Plasmas},
keywords = {deuterium,fusion reactor fuel,hydrodynamics,laser fusion,plasma instability,plasma simulation,tritium},
month = {may},
number = {5},
pages = {056302},
publisher = {AIP Publishing LLC},
title = {{Three-dimensional simulations of low foot and high foot implosion experiments on the National Ignition Facility}},
